import {
}

extern "C" {
    // This isn't the actual printf signature, but we won't have variadic functions
    fn printf(str &CStr, message &CStr, total Int) Int;
}

fn main() Int {
    calc(&c"testing calculation");
}

fn add(num1 Int, num2 Int) Int {
    num1 + num2
}

fn calc(message &CStr) Int {
    let x = 5;
    let y = x + 2;
    let z = add(x, y + 5);
    printf(&c"%s z: %d\n", message, z)
}
